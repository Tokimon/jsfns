{
  "name": "@js-fns/core",
  "version": "1.0.0",
  "description": "Simple, no dependency, JS utility functions",
  "author": "Toke Voltelen",
  "license": "MIT",
  "type": "module",
  "main": "./index.js",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "typings": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://Tokimon@github.com/Tokimon/js-fns.git"
  },
  "keywords": [
    "javascript",
    "helpers",
    "snippets",
    "utility",
    "tool"
  ],
  "bugs": {
    "url": "https://github.com/Tokimon/js-fns/issues"
  },
  "homepage": "https://github.com/Tokimon/js-fns#readme",
  "scripts": {
    "build": "../../bin/build.sh",
    "clean": "../../bin/clean.sh",
    "missing": "../../bin/missing.sh",
    "docs-old": "../../bin/docs.sh",
    "docs": "npm run --prefix ../../docs-builder build core",
    "types": "tsc -p tsconfig.json",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:coverage": "npm t -- --collectCoverage",
    "lint": "eslint . --max-warnings=0",
    "validate": "pnpm lint && pnpm types && pnpm test"
  },
  "directories": {
    "test": "tests"
  },
  "devDependencies": {
    "@types/jest": "~29.5.2",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.43.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "~29.5.0",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.24.8",
    "typedoc-plugin-missing-exports": "^2.0.0",
    "typescript": "^5.1.3"
  }
}
