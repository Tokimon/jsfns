{
  "name": "@jsfns/core",
  "version": "1.0.0",
  "description": "Simple, no dependency, JS utility functions",
  "private": false,
  "author": "Toke Voltelen",
  "license": "MIT",
  "type": "module",
  "main": "./index.js",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "typings": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://Tokimon@github.com/Tokimon/jsfns.git"
  },
  "keywords": [
    "javascript",
    "helpers",
    "snippets",
    "utility",
    "tool"
  ],
  "bugs": {
    "url": "https://github.com/Tokimon/jsfns/issues"
  },
  "homepage": "https://tokimon.github.io/jsfns/core",
  "scripts": {
    "prepublishOnly": "pnpm run test && pnpm run build && pnpm run docs",
    "postpublish": "pnpm run clean",
    "build": "../../bin/build.sh",
    "clean": "../../bin/clean.sh",
    "missing": "../../bin/missing.sh",
    "docs": "npm run --prefix ../../docs-builder build core",
    "types": "tsc -p tsconfig.json",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:coverage": "npm t -- --collectCoverage",
    "lint": "eslint . --max-warnings=0",
    "prettier": "prettier --write .",
    "validate": "pnpm lint && pnpm types && pnpm test"
  },
  "directories": {
    "test": "tests"
  },
  "devDependencies": {
    "@types/jest": "~29.5.2",
    "@types/node": "^20.3.1",
    "jest": "~29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  }
}
