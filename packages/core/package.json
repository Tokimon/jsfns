{
  "name": "@jsfns/core",
  "version": "1.2.0",
  "description": "Simple, no dependency, JS utility functions",
  "private": false,
  "author": "Toke Voltelen",
  "license": "MIT",
  "type": "module",
  "main": "./index.js",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "typings": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://Tokimon@github.com/Tokimon/jsfns.git"
  },
  "keywords": [
    "javascript",
    "helpers",
    "snippets",
    "utility",
    "tool"
  ],
  "bugs": {
    "url": "https://github.com/Tokimon/jsfns/issues"
  },
  "homepage": "https://tokimon.github.io/jsfns-docs/core",
  "scripts": {
    "prepublishOnly": "pnpm test:coverage:json && pnpm build && pnpm readme",
    "postpublish": "pnpm clean",
    "dry-run": "pnpm build && npm pack --dry-run && pnpm clean",
    "build": "../../bin/build.sh",
    "clean": "../../bin/clean.sh",
    "missing": "../../bin/missing.sh",
    "readme": "esrun ../../readme-builder/build.ts",
    "types": "tsc -p tsconfig.json",
    "test": "jest",
    "test:coverage:json": "npm t -- --coverage",
    "test:coverage:cli": "npm t -- --coverage --coverageReporters='text'",
    "lint": "biome lint --error-on-warnings",
    "format": "biome format --write",
    "format:check": "biome format",
    "validate": "pnpm lint && pnpm format:check && pnpm types && pnpm test:coverage:cli",
    "release": "pnpm publish"
  },
  "devDependencies": {
    "@types/jest": "~29.5.14",
    "@types/node": "^22.14.1",
    "jest": "~29.7.0",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3"
  },
  "files": [
    "/*.js",
    "/*.cjs",
    "/*.mjs",
    "/*.d.ts",
    "/README.md",
    "/LICENSE",
    "!/jest.*.js",
    "!/readme/*"
  ]
}
