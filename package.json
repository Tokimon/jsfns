{
  "name": "js-fns",
  "version": "1.0.0",
  "description": "Simple, no dependency, JS utility functions",
  "main": "index.js",
  "private": true,
  "type": "module",
  "scripts": {
    "prepublishOnly": "npm run build",
    "postpublish": "npm run clean",
    "test": "npm test --workspaces",
    "test:core": "npm test -w @js-fns/core",
    "test:web": "npm test -w @js-fns/web",
    "test:coverage": "npm run test:coverage --workspaces",
    "types": "npm run types --workspaces",
    "types:core": "npm run types -w @js-fns/core",
    "types:web": "npm run types -w @js-fns/web",
    "build": "npm run build --workspaces",
    "build:core": "npm run build -w @js-fns/core",
    "build:web": "npm run build -w @js-fns/web",
    "clean": "npm run clean --workspaces",
    "clean:core": "npm run clean -w @js-fns/core",
    "clean:web": "npm run clean -w @js-fns/web",
    "missing": "npm run missing --workspaces",
    "missing:core": "npm run missing -w @js-fns/core",
    "missing:web": "npm run missing -w @js-fns/web",
    "lint": "npm run lint --workspaces",
    "lint:core": "npm run lint -w @js-fns/core",
    "lint:web": "npm run lint -w @js-fns/web",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "coveralls:win": "type ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "docs": "npm run docs --workspaces",
    "docs:core": "npm run docs -w @js-fns/core",
    "docs:web": "npm run docs -w @js-fns/web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tokimon/js-fns.git"
  },
  "keywords": [],
  "author": "Toke Voltelen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tokimon/js-fns/issues"
  },
  "homepage": "https://github.com/Tokimon/js-fns#readme",
  "devDependencies": {}
}
